<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Smoko.Properties"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="ApplicationName">Smoko</system:String>
    <system:String x:Key="ApplicationSlogan">Gather people together</system:String>
    <system:String x:Key="MessageDefaultCaption">Smoko says...</system:String>
    <system:String x:Key="MessageAppAlreadyRunning" xml:space="preserve">Smoko is already running.&#13;You can find its icon in system tray.</system:String>
</ResourceDictionary>